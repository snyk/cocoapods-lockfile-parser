{
  "name": "snyk-cocoapods-lockfile-parser",
  "description": "Generate a Snyk dependency tree from a Podfile.lock file",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && npm run unit-test",
    "unit-test": "tap test/lib -R=spec --timeout=300 --node-path ts-node --test-file-pattern '/\\.[tj]s$/'",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-cocoapods-lockfile-parser"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/snyk/snyk-cocoapods-lockfile-parser#readme",
  "dependencies": {
    "lodash": "^4.17.11",
    "source-map-support": "^0.5.7",
    "tslib": "^1.9.3",
    "snyk": "^1.192.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "@types/node": "^4.0.47",
    "tap": "github:snyk/node-tap#alternative-runtimes",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  },
  "snyk": true
}
