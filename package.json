{
  "name": "snyk-cocoapods-lockfile-parser",
  "description": "Generate a Snyk dependency tree from a Podfile.lock file",
  "main": "dist/index.js",
  "scripts": {
    "format:check": "prettier --check '{lib,test}/**/*.{js,ts}'",
    "format": "prettier --write '{lib,test}/**/*.{js,ts}'",
    "lint": "npm run format:check && tslint -p tsconfig.json",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "tap test/lib -R=spec --timeout=300 --node-path ts-node --test-file-pattern '/\\.[tj]s$/'",
    "prepare": "npm run build",
    "build": "tsc",
    "build-watch": "tsc -w",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-cocoapods-lockfile-parser"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/snyk/snyk-cocoapods-lockfile-parser#readme",
  "dependencies": {
    "lodash": "^4.17.11",
    "snyk": "^1.192.1",
    "source-map-support": "^0.5.7",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "@types/node": "^4.0.47",
    "husky": "^3.0.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tap": "github:snyk/node-tap#alternative-runtimes",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
